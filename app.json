{
  "name": "AUBLIC Extractor Bot",
  "description": "Telegram bot to extract and save restricted course content. Powered by MADX & AUBLIC.",
  "repository": "https://github.com/devgaganin/save_restricted-content-telegram-bot-repo",
  "keywords": ["telegram", "bot", "pyrogram", "aulbic", "content-extractor"],
  "env": {
    "API_ID": {
      "description": "Telegram API ID from https://my.telegram.org",
      "required": true
    },
    "API_HASH": {
      "description": "Telegram API HASH from https://my.telegram.org",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Telegram Bot Token from @BotFather",
      "required": true
    },
    "OWNER_ID": {
      "description": "Your Telegram numeric user ID (e.g., 7360968885)",
      "required": true
    },
    "MONGO_DB": {
      "description": "MongoDB connection URI (e.g., from MongoDB Atlas)",
      "required": true
    },
    "LOG_GROUP": {
      "description": "Log group/channel ID (optional)",
      "required": false
    },
    "CHANNEL_ID": {
      "description": "Private channel ID for forwarding content",
      "required": false
    },
    "FREEMIUM_LIMIT": {
      "description": "Daily limit for free users",
      "required": false
    },
    "PREMIUM_LIMIT": {
      "description": "Daily limit for premium users",
      "required": false
    },
    "STRING": {
      "description": "Pyrogram session string (optional)",
      "required": false
    },
    "AD_API": {
      "description": "Optional ad API URL",
      "required": false
    },
    "YT_COOKIES": {
      "description": "YouTube cookies if needed",
      "required": false
    },
    "DEFAUL_SESSION": {
      "description": "Old invite session string",
      "required": false
    },
    "INSTA_COOKIES": {
      "description": "Instagram cookies for scraping",
      "required": false
    }
  }
}
